{
    "routes": [
      {
        "route": "/hello",
        "redirect": "/",
        "statusCode": 301
      },
      {
        "route": "/bob",
        "rewrite": "/api/home"
      },
      {
        "route": "/callback",
        "redirect": "/api/callback"
      },
      {
        "route": "/success",
        "redirect": "/api/success"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }